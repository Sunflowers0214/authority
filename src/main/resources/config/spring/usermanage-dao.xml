<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- 全局参数 -->
    <bean id="configDao" class="com.flysnow.usermanage.dao.impl.ConfigDaoImpl">
        <property name="sqlSessionFactory" ref="sqlSessionFactoryUsermanage"/>
    </bean>

    <!-- 部门 -->
    <bean id="departmentDao" class="com.flysnow.usermanage.dao.impl.DepartmentDaoImpl">
        <property name="sqlSessionFactory" ref="sqlSessionFactoryUsermanage"/>
    </bean>

    <!-- 字典 -->
    <bean id="dictDao" class="com.flysnow.usermanage.dao.impl.DictDaoImpl">
        <property name="sqlSessionFactory" ref="sqlSessionFactoryUsermanage"/>
    </bean>

    <!-- 字典选项 -->
    <bean id="dictItemDao" class="com.flysnow.usermanage.dao.impl.DictItemDaoImpl">
        <property name="sqlSessionFactory" ref="sqlSessionFactoryUsermanage"/>
    </bean>

    <!-- 登录日志 -->
    <bean id="loginLogDao" class="com.flysnow.usermanage.dao.impl.LoginLogDaoImpl">
        <property name="sqlSessionFactory" ref="sqlSessionFactoryUsermanage"/>
    </bean>

    <!-- 菜单 -->
    <bean id="menuDao" class="com.flysnow.usermanage.dao.impl.MenuDaoImpl">
        <property name="sqlSessionFactory" ref="sqlSessionFactoryUsermanage"/>
    </bean>

    <!-- 菜单权限 -->
    <bean id="menuPermissionDao" class="com.flysnow.usermanage.dao.impl.MenuPermissionDaoImpl">
        <property name="sqlSessionFactory" ref="sqlSessionFactoryUsermanage"/>
    </bean>

    <!-- 操作日志 -->
    <bean id="operateLogDao" class="com.flysnow.usermanage.dao.impl.OperateLogDaoImpl">
        <property name="sqlSessionFactory" ref="sqlSessionFactoryUsermanage"/>
    </bean>

    <!-- 角色 -->
    <bean id="roleDao" class="com.flysnow.usermanage.dao.impl.RoleDaoImpl">
        <property name="sqlSessionFactory" ref="sqlSessionFactoryUsermanage"/>
    </bean>

    <!-- 角色功能 -->
    <bean id="roleFuncDao" class="com.flysnow.usermanage.dao.impl.RoleFuncDaoImpl">
        <property name="sqlSessionFactory" ref="sqlSessionFactoryUsermanage"/>
    </bean>

    <!-- 角色菜单 -->
    <bean id="roleMenuDao" class="com.flysnow.usermanage.dao.impl.RoleMenuDaoImpl">
        <property name="sqlSessionFactory" ref="sqlSessionFactoryUsermanage"/>
    </bean>

    <!-- 用户 -->
    <bean id="userDao" class="com.flysnow.usermanage.dao.impl.UserDaoImpl">
        <property name="sqlSessionFactory" ref="sqlSessionFactoryUsermanage"/>
    </bean>

    <!-- 用户登录权限 -->
    <bean id="userPermissionDao" class="com.flysnow.usermanage.dao.impl.UserPermissionDaoImpl">
        <property name="sqlSessionFactory" ref="sqlSessionFactoryUsermanage"/>
    </bean>

    <!-- 用户角色 -->
    <bean id="userRoleDao" class="com.flysnow.usermanage.dao.impl.UserRoleDaoImpl">
        <property name="sqlSessionFactory" ref="sqlSessionFactoryUsermanage"/>
    </bean>

</beans>
